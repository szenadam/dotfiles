set nocompatible
filetype off
filetype plugin indent on
syntax on
let mapleader = ","
set modelines=0
set number
set ruler
set visualbell
set encoding=utf-8
set wrap
set textwidth=119
set formatoptions=tcqrn1
set tabstop=2
set shiftwidth=2
set softtabstop=2
set expandtab
set noshiftround
set scrolloff=3
set backspace=indent,eol,start
set hidden
set ttyfast
set laststatus=2
set showmode
set showcmd


